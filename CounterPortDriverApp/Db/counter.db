## Counter.db

record(ai, "$(USER):get:set:one")
{
    field(DTYP, "asynFloat64")
    field(INP, "@asyn($(PORT),$(ADDR),$(TIMEOUT))COUNTER")
    field(SCAN, "I/O Intr")
  }

